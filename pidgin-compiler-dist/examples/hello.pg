// Simple arithmetic and variables
let x = 10;
let y = 20;
let sum = x + y;
print sum;

// String operations
let greeting = "Hello, ";
let name = "World!";
print greeting + name;

// Conditional
if (sum > 25) {
    print "Sum is greater than 25";
} else {
    print "Sum is not greater than 25";
}

// Loop
let counter = 0;
while (counter < 3) {
    print "Counter: " + counter;
    counter = counter + 1;
}
